<%- include('partials/header'); %>
<!-- Header section
================================================== -->
<section id="header" class="header-four" style="background-image:url('/images/disney1.jpg')">
	<div class="container">
		<div class="row">

			<div class="col-md-offset-3 col-md-6 col-sm-offset-2 col-sm-8">
				<div class="header-thumb">
					<h1 class="wow fadeIn" data-wow-delay="0.6s">Contact Us</h1>
					<h3 class="wow fadeInUp" data-wow-delay="0.9s">For more information check link below.</h3>
				</div>
			</div>

		</div>
	</div>
</section>


<!-- Contact section
================================================== -->
<section id="contact" style="background: #ebebff">
	<div class="container">
		<div class="row">

			<div class="clearfix"></div>

			<div class="col-md-4 col-sm-4">
				<div class="wow fadeInUp media" data-wow-delay="0.6s">
					<div class="media-object pull-left">
						<i class="fa fa-instagram"></i>
					</div>
					<div class="media-body">
						<h3 class="media-heading">Instagram</h3>
						<p>@KIDDy.Play</p>
					</div>
				</div>
			</div>

			<div class="col-md-4 col-sm-4">
				<div class="wow fadeInUp media" data-wow-delay="0.6s">
					<div class="media-object pull-left">
						<i class="fa fa-twitter"></i>
					</div>
					<div class="media-body">
						<h3 class="media-heading">Twitter</h3>
						<p>@KIDDy.Play</p>
					</div>
				</div>
			</div>

			<div class="col-md-4 col-sm-4">
				<div class="wow fadeInUp media" data-wow-delay="0.6s">
					<div class="media-object pull-left">
						<i class="fa fa-facebook"></i>
					</div>
					<div class="media-body">
						<h3 class="media-heading">Facebook</h3>
						<p>KIDDY PLAY</p>
					</div>
				</div>
			</div>

		</div>
	</div>
</section>


